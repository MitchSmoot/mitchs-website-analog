{
  "name": "mitchs-website",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/mitchs-website/src",
  "targets": {
    "build": {
      "executor": "@analogjs/platform:vite",
      "outputs": [
        "{options.outputPath}",
        "{workspaceRoot}/dist/apps/mitchs-website/.nitro",
        "{workspaceRoot}/dist/apps/mitchs-website/ssr",
        "{workspaceRoot}/dist/apps/mitchs-website/analog"
      ],
      "options": {
        "main": "apps/mitchs-website/src/main.ts",
        "configFile": "apps/mitchs-website/vite.config.ts",
        "outputPath": "dist/apps/mitchs-website/client",
        "tsConfig": "apps/mitchs-website/tsconfig.app.json"
      },
      "defaultConfiguration": "production",
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "sourcemap": false,
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@analogjs/platform:vite-dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "mitchs-website:build",
        "port": 4200
      },
      "configurations": {
        "development": {
          "buildTarget": "mitchs-website:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "mitchs-website:build:production"
        }
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "mitchs-website:build"
      }
    },
    "test": {
      "executor": "@analogjs/platform:vitest",
      "outputs": ["{projectRoot}/coverage"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/mitchs-website/**/*.{ts,html}"]
      }
    }
  },
  "tags": []
}
