{
  "name": "mitchs-website",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "mitchs",
  "sourceRoot": "apps/mitchs-website/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}",
        "dist/apps/mitchs-website/.nitro",
        "dist/apps/mitchs-website/ssr",
        "dist/apps/mitchs-website/analog"
      ],
      "options": {
        "configFile": "apps/mitchs-website/vite.config.ts",
        "outputPath": "dist/apps/mitchs-website",
        "assets": [
          "apps/mitchs-website/src/favicon.ico",
          "apps/mitchs-website/src/assets"
        ],
        "styles": [
          "apps/mitchs-website/src/styles.scss",
          "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": [
            {
              "replace": "apps/mitchs-website/src/environments/environment.ts",
              "with": "apps/mitchs-website/src/environments/environment.development.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "mitchs-website:build",
        "port": 4200
      },
      "configurations": {
        "production": {
          "browserTarget": "mitchs-website:build:production"
        },
        "development": {
          "browserTarget": "mitchs-website:build:development"
        }
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "mitchs-website:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/mitchs-website/**/*.ts",
          "apps/mitchs-website/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/mitchs-website/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "mitchs-website:build"
      }
    }
  }
}